{
  "name": "portfolio-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@devloco/react-scripts-wptheme": "3.4.0-wp.1",
    "@material-ui/core": "^4.9.5",
    "@material-ui/icons": "^4.9.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "axios": "^0.19.2",
    "fg-loadcss": "^2.1.0",
    "node-sass-chokidar": "^1.4.0",
    "npm-run-all": "4.1.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-router-dom": "^5.1.2"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "crabuild": "react-scripts build",
    "craeject": "react-scripts eject",
    "crastart": "react-scripts start",
    "cratest": "react-scripts test",
    "build": "wptheme-scripts build",
    "start": "wptheme-scripts start",
    "wpbuild": "wptheme-scripts wpbuild",
    "wpstart": "wptheme-scripts wpstart"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "homepage": "/portfolio-react/wp-content/themes/portfolio",
  "browserLaunchTo": "http://localhost/portfolio-react",
  "devDependencies": {
    "compass-mixins": "^0.12.10"
  }
}
